{
  "name": "todo-challenge",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "NODE_ENV='production' node dist/main",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --silent --watch",
    "test:staged": "yarn test --silent --noStackTrace --findRelatedTests",
    "test:coverage": "yarn test --silent --noStackTrace --coverage",
    "test:integration": "yarn test -c ./jest.integration.config.js",
    "typeorm": "ts-node-dev  -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:ci": "yarn test --silent --noStackTrace --coverage",
    "check:dep": "yarn-check -s -u"
  },
  "keywords": [],
  "author": "Caio Vieira",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.1",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-pino-logger": "7.0.0",
    "module-alias": "2.2.2",
    "pg": "8.7.3",
    "pino": "7.11.0",
    "pino-http": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "swagger-ui-express": "4.3.0",
    "typeorm": "0.2.29",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@jest-mock/express": "1.4.5",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-pino-logger": "4.0.3",
    "@types/jest": "27.4.1",
    "@types/module-alias": "2.0.1",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.23",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.2",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-mock-extended": "2.0.4",
    "lint-staged": "12.3.7",
    "pg-mem": "2.3.5",
    "pino-pretty": "7.6.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": "16.x"
  }
}
